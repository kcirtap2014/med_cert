		<script type="text/javascript">
			var nbs = 2;
			// nbs est le nombre d'image qui compose le slide de bas de page.

			var slide_speed = 500;
			var taille_img_slider=468;
			var online_left=online_right=0;

			function set_taille_slider(){
				$taille_calc_s = (nbs*taille_img_slider);
				$('#slide-center-conteneur ul').css('width',$taille_calc_s+'px');

			}

			function animate_fn_slide_left(){
				if (online_left==0 && $('#slide-center ul li').length>1)
				{
					online_left=1;
					$('#slide-center ul li:first').before($('#slide-center ul li:last'));
					$('#slide-center ul').css('left',"-"+taille_img_slider+"px");
					$('#slide-center ul').animate({left:0,easing:"swing"},slide_speed,function()
					{
						online_left=0;
					});
				}
			}
			function animate_fn_slide_right(){
				if (online_right==0 && $('#slide-center ul li').length>1)
				{
					online_right=1;
					$('#slide-center ul').animate({left:"-"+taille_img_slider,easing:"swing"},slide_speed,function()
					{
						$('#slide-center ul li:last').after($('#slide-center ul li:first'));
						$('#slide-center ul').css('left',"0px");
						online_right=0;
					});
				}
			}

			function animate_fn_news(){
				$.post("index_news.php", {exclure: $('#jq_actu').val()}, function(data){
					$('#contenu-bottom-right-news').hide();
					$('#contenu-bottom-right-news').html(data);
					$('#contenu-bottom-right-news').show('slow');
					setTimeout('animate_fn_news()',5000);
				});
			}

			function animate_bann(){
				animate_fn_slide_right();
				setTimeout('animate_bann()',20000);
			}

			$(document).ready(function(){
				set_taille_slider();

				$('#arr-left').click(function(){
					animate_fn_slide_left();
				});
				$('#arr-right').click(function(){
					animate_fn_slide_right();
				});

				setTimeout('animate_fn_news()',3000);

				setTimeout('animate_bann()',20000);
			});

		</script>
